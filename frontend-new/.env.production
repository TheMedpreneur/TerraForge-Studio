# Production Environment Variables
# TerraForge Studio v4.x

# API Configuration
VITE_API_URL=https://api.terraforge.studio
VITE_WS_URL=wss://api.terraforge.studio/ws

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_AI_FEATURES=false

# Map Providers
VITE_MAPBOX_TOKEN=
VITE_GOOGLE_MAPS_KEY=

# Analytics
VITE_GA_TRACKING_ID=
VITE_MIXPANEL_TOKEN=

# Sentry (Error Tracking)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=production

# App Configuration
VITE_APP_VERSION=4.0.0
VITE_APP_NAME=TerraForge Studio

# Performance
VITE_MAX_CACHE_SIZE=100
VITE_MAX_HISTORY_ITEMS=50
